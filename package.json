{
  "name": "ionic-culturemix",
  "version": "1.0.0",
  "author": "Technologiestiftung Berlin",
  "homepage": "https://www.technologiestiftung-berlin.de/de/startseite/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve -b",
    "start:hacking": "ionic serve -b -- -c=hacking",
    "start:staging": "ionic serve -b -- -c=staging",
    "start:production": "ionic serve -b -- -c=production",
    "build": "ng build",
    "lint": "ng lint",
    "lint:staged": "node scripts/lint-staged.js --no-hard-exit",
    "check-dependencies": "node scripts/dependency-checker.js",
    "order-css": "node scripts/css-order.js",
    "order-css:fix": "node scripts/css-order.js -f",
    "order-css:husky": "node scripts/css-order.js -h"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-dependencies && node scripts/lint-staged.js"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.9",
    "@angular/cdk": "8.2.3",
    "@angular/common": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/material": "8.2.3",
    "@angular/material-moment-adapter": "8.2.3",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/pwa": "0.803.8",
    "@angular/router": "9.1.9",
    "@angular/service-worker": "9.1.9",
    "@ionic/angular": "4.11.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/mapbox-gl": "1.3.0",
    "core-js": "3.2.1",
    "mapbox-gl": "1.5.0",
    "moment": "2.24.0",
    "ngx-json-viewer": "2.4.0",
    "ngx-moment": "3.4.0",
    "rxjs": "6.5.3",
    "sw-toolbox": "3.6.0",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.803.8",
    "@angular-devkit/build-angular": "0.803.8",
    "@angular-devkit/core": "8.3.8",
    "@angular-devkit/schematics": "8.3.8",
    "@angular/cli": "8.3.13",
    "@angular/compiler": "9.1.9",
    "@angular/compiler-cli": "9.1.9",
    "@angular/language-service": "9.1.9",
    "@ionic/angular-toolkit": "2.0.0",
    "@types/node": "12.7.12",
    "codelyzer": "5.1.2",
    "eslint": "6.5.1",
    "husky": "3.0.8",
    "staged-git-files": "1.2.0",
    "stylelint": "11.0.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-order": "3.1.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.5.3"
  },
  "description": "0.1.0"
}
